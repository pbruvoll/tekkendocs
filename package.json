{
  "name": "tekkendocs",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "build:sitemap": "remix-sitemap",
    "check": "biome check",
    "check:fix": "biome check --fix",
    "dev": "node ./server.js",
    "dev-mock-data": "cross-env USE_MOCK_DATA=1 remix dev --manual -c \"node server.js\"",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "setup": "prisma generate && prisma migrate deploy",
    "setup:db:dev": "prisma migrate reset && prisma migrate dev",
    "test": "vitest",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "lint-biome": "biome lint",
    "lint-biome-errors": "biome lint --diagnostic-level=error",
    "lint-biome:fix": "biome lint --fix",
    "format": "prettier --write .",
    "formatcheck": "prettier --check .",
    "format-biome": "biome format .",
    "format-biome:fix": "biome format --write",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/themes": "^3.2.1",
    "@remix-run/express": "2.16.6",
    "@remix-run/node": "2.16.6",
    "@remix-run/react": "2.16.6",
    "cachified": "^3.5.4",
    "class-variance-authority": "^0.7.0",
    "classix": "^2.1.35",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "isbot": "^4.1.0",
    "lru-cache": "^10.0.1",
    "lucide-react": "^0.396.0",
    "morgan": "^1.10.0",
    "motion": "^12.23.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-player": "^2.16.0",
    "remix-utils": "^7.6.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@remix-run/dev": "2.16.6",
    "@remix-run/eslint-config": "2.16.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "prisma": "^5.18.0",
    "remix-development-tools": "^4.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
