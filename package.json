{
  "name": "tekkendocs",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "build:sitemap": "remix-sitemap",
    "dev": "remix dev --manual -c \"node server.js\"",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "formatcheck": "prettier --check .",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.0",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/express": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "cachified": "^3.5.4",
    "chokidar": "^3.5.3",
    "classix": "^2.1.35",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "isbot": "^3.6.8",
    "lru-cache": "^10.0.1",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-sitemap": "^3.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.44.8",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/source-map-support": "^0.5.6",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
